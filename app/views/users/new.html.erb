<h1>Sign Up for New Users!</h1>

<%= form_for(@user, url:'/signup') do |f| %>
    
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <%= f.submit "Submit" %>

<% end %>